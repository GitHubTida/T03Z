    <rules>
        <fullName>SSO OPT PIC Request</fullName>
        <actions>
            <name>Provisioned</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Set Provisioned Date</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <booleanFilter>1 AND 2 AND 3 AND 4 AND 5</booleanFilter>
        <criteriaItems>
            <field>SSO_Workflow__c.User_Type__c</field>
            <operation>equals</operation>
            <value>Partner</value>
        </criteriaItems>
        <criteriaItems>
            <field>SSO_Workflow__c.Application_Name__c</field>
            <operation>equals</operation>
            <value>Order Provisioning Tool</value>
