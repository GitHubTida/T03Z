        <criteriaItems>
            <field>SSO_Workflow__c.Request_Type__c</field>
            <operation>equals</operation>
            <value>Application Grant Access</value>
        </criteriaItems>
        <criteriaItems>
            <field>SSO_Workflow__c.Process_Status__c</field>
            <operation>equals</operation>
            <value>Requested</value>
        </criteriaItems>
        <description>This workflow rule is written to provision the Order Provisioning Tool access requested by PIC.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
