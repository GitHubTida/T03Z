        <criteriaItems>
            <field>SSO_Email_Out__c.Email_Address__c</field>
            <operation>contains</operation>
            <value>@zebra.com</value>
        </criteriaItems>
        <criteriaItems>
            <field>SSO_Email_Out__c.Email_Address__c</field>
            <operation>contains</operation>
            <value>@gmail.com</value>
        </criteriaItems>
        <triggerType>onCreateOnly</triggerType>
    </rules>
