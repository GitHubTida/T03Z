        <actions>
            <name>Email Sent Flag Update</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <booleanFilter>1 AND (2 OR 3 OR 4)</booleanFilter>
        <criteriaItems>
            <field>SSO_Email_Out__c.Type__c</field>
            <operation>equals</operation>
            <value>Generate Token</value>
        </criteriaItems>
        <criteriaItems>
            <field>SSO_Email_Out__c.Email_Address__c</field>
            <operation>contains</operation>
            <value>@mailinator.com</value>
