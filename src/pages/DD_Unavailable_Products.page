<apex:page standardcontroller="DD_Backlog_Item__c" extensions="DD_Unavailable_Products">
    <apex:sectionHeader title="Unavailable Products Edit" subtitle="New Unavailable Products"/>
  <apex:form >
   <apex:pageBlock title="Unavailable Products Edit" mode="edit">
   <apex:pageBlockButtons >
       <apex:commandButton action="{!savemethod}" value="Save"/>
       <apex:commandButton value="Cancel" action="{!Cancel}"/>
   </apex:pageBlockButtons>    
  <apex:pageBlockSection title="Information" collapsible="false" columns="1">
       <apex:inputField value="{!unavailableproducts.Demo_Request__c}"/>
       <apex:inputField value="{!unavailableproducts.Material__c}"/>
       <apex:inputField value="{!unavailableproducts.Quantity__c}"/>
  </apex:pageBlockSection>
  </apex:pageBlock>
  </apex:form>
</apex:page>