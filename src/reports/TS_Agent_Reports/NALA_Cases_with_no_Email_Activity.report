    <description>This report will show all cases where the TS agent has sent an email to the customer, but the customer has not yet responded within 48 hours (2880 minutes).These reports are eligible to be soft closed.</description>
    <filter>
        <criteriaItems>
            <column>STATUS</column>
            <operator>contains</operator>
            <value>Working,Pending Customer Response</value>
        </criteriaItems>
        <criteriaItems>
            <column>Case.Last_Email_Status__c</column>
            <operator>equals</operator>
            <value>Sent</value>
        </criteriaItems>
        <criteriaItems>
            <column>Case.ZEB_Time_Since_Last_Email_Sent__c</column>
            <operator>greaterThan</operator>
            <value>2880</value>
        </criteriaItems>
    </filter>
    <format>Tabular</format>
    <name>NALA Cases with no Email Activity</name>
    <params>
        <name>units</name>
        <value>h</value>
    </params>
    <params>
        <name>co</name>
        <value>1</value>
    </params>
    <reportType>CaseList</reportType>
    <scope>organization</scope>
    <showDetails>true</showDetails>
    <timeFrameFilter>
        <dateColumn>CREATED_DATEONLY</dateColumn>
        <interval>INTERVAL_CUSTOM</interval>
    </timeFrameFilter>
</Report>
