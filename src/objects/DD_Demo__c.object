    <validationRules>
        <fullName>DD_Reason_for_Code_mendatory</fullName>
        <active>true</active>
        <errorConditionFormula>AND($Profile.Name!=&apos;System Administrator&apos;,ISPICKVAL( Reason_Code__c,&quot;&quot;))</errorConditionFormula>
        <errorMessage>&quot;Reason code&quot; is mandatory</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DD_Reason_of_Demo_is_required</fullName>
        <active>true</active>
        <description>Reason of demo is mandatory, if Reason Code is Others</description>
        <errorConditionFormula>AND($Profile.Name!=&apos;System Administrator&apos;,ISPICKVAL(Reason_Code__c,&apos;Others&apos;), ISBLANK(Reason_for_Demo__c) )</errorConditionFormula>
        <errorMessage>Reason of demo is mandatory, if Reason Code is Others</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DD_state_mendatory</fullName>
        <active>true</active>
        <description>state is mendatory when country is USA and Canada</description>
        <errorConditionFormula>AND($Profile.Name!=&apos;System Administrator&apos;,OR( Country__c=&apos;United States&apos;, Country__c=&apos;Canada&apos;), ISBLANK( State_Province__c ))</errorConditionFormula>
        <errorMessage>State is mandatory when country selected is USA OR CANADA</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>Accept_and_Submit</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Accept and Submit</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/24.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/24.0/apex.js&quot;)} 

var validSubmitUser= sforce.apex.execute(&quot;DD_ValidateButtonClick&quot;,&quot;isValidSubmitUser&quot;,{demoRequestId:&quot;{!DD_Demo__c.Id}&quot;});

if(validSubmitUser==&apos;false&apos;){
alert(&apos;You cannot submit this request for approval&apos;);
}
else if (&apos;{!DD_Demo__c.Demo_Status__c}&apos; != &apos;Awaiting AM Submission&apos;) {
alert(&apos;Demo Request status must be Awaiting AM Submission to submit for approval&apos;);

}else if(&apos;{!DD_Demo__c.Parts_Reserved__c}&apos; == 0){
alert(&apos;Select parts before submitting for approval&apos;);

}else if(&apos;{!DD_Demo__c.Count_Address__c}&apos; == 0){
alert(&apos;Select Shipping Address before submitting for approval&apos;);
}else{
if(&apos;{!DD_Demo__c.Opportunity__c}&apos;==&apos;&apos;){
alert(&apos;You have chosen to submit a demo request without an opportunity. This will require additional approval steps and review from the program administrator&apos;);
}
var result = sforce.apex.execute(&quot;DD_ValidateButtonClick&quot;,&quot;submitForApproval&quot;,{demoRequestId:&quot;{!DD_Demo__c.Id}&quot;});

if(result){
window.open(&apos;/{!DD_Demo__c.Id}&apos;,&apos;_top&apos;); 
}
}</url>
    </webLinks>
