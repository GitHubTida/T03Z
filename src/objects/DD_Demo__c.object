        <fullName>Action__c</fullName>
        <formula>IF(  Theater__c == &apos;NA&apos; &amp;&amp; (Today() &gt;  Evaulation_End_Date__c + 30),

CASE( Demo_Status__c ,
&apos;Awaiting AM Submission&apos;,  IF(ISPICKVAL(Approval_Status__c,&apos;Rejected&apos;),&apos;Rejected&apos; ,&apos;Not Submitted&apos;), 
&apos;Awaiting SE Validation&apos;, &apos;Pending Approval&apos;,
&apos;Awaiting Manager Approval&apos;,&apos;Pending Approval&apos;,
&apos;Awaiting Admin Approval&apos;,&apos;Pending Approval&apos;,
&apos;Awaiting Service Action&apos;,IF(ISBLANK(Picked_Date__c ) ,&apos;With Services&apos;,&apos;Picked&apos;),
&apos;In Progress with Services&apos;,IF(ISBLANK(Picked_Date__c ) ,&apos;With Services&apos;,&apos;Picked&apos;),
&apos;Shipped&apos;,&apos;Awaiting Write Off&apos;,
&apos;Return In Progress&apos;,&apos;Awaiting Write Off&apos;,
&apos;Closed&apos;,IF(  Write_Off_Count__c &gt; 0,&apos;Write Off&apos;,&apos;Closed&apos;),
&apos;Closed&apos;
)

,  IF(Theater__c != &apos;NA&apos; &amp;&amp; (Today() &gt; Evaulation_End_Date__c),

CASE( Demo_Status__c ,
&apos;Awaiting AM Submission&apos;,  IF(ISPICKVAL(Approval_Status__c,&apos;Rejected&apos;),&apos;Rejected&apos; ,&apos;Not Submitted&apos;), 
&apos;Awaiting SE Validation&apos;, &apos;Pending Approval&apos;,
&apos;Awaiting Manager Approval&apos;,&apos;Pending Approval&apos;,
&apos;Awaiting Admin Approval&apos;,&apos;Pending Approval&apos;,
&apos;Awaiting Service Action&apos;,IF(ISBLANK(Picked_Date__c ) ,&apos;With Services&apos;,&apos;Picked&apos;),
&apos;In Progress with Services&apos;,IF(ISBLANK(Picked_Date__c ) ,&apos;With Services&apos;,&apos;Picked&apos;),
&apos;Shipped&apos;,&apos;Awaiting Write Off&apos;,
&apos;Return In Progress&apos;,&apos;Awaiting Write Off&apos;,
&apos;Closed&apos;,IF(  Write_Off_Count__c &gt; 0,&apos;Write Off&apos;,&apos;Closed&apos;),
&apos;Closed&apos;
), 
CASE( Demo_Status__c ,
&apos;Awaiting AM Submission&apos;,  IF(ISPICKVAL(Approval_Status__c,&apos;Rejected&apos;),&apos;Rejected&apos; ,&apos;Not Submitted&apos;), 
&apos;Awaiting SE Validation&apos;, &apos;Pending Approval&apos;,
&apos;Awaiting Manager Approval&apos;,&apos;Pending Approval&apos;,
&apos;Awaiting Admin Approval&apos;,&apos;Pending Approval&apos;,
&apos;Awaiting Service Action&apos;,IF(ISBLANK(Picked_Date__c ) ,&apos;With Services&apos;,&apos;Picked&apos;),
&apos;In Progress with Services&apos;,IF(ISBLANK(Picked_Date__c ) ,&apos;With Services&apos;,&apos;Picked&apos;),
&apos;Shipped&apos;,&apos;Shipped&apos;,
&apos;Return In Progress&apos;,&apos;Return In Progress&apos;,
&apos;Closed&apos;,IF(  Write_Off_Count__c &gt; 0,&apos;Write Off&apos;,&apos;Closed&apos;),
&apos;Closed&apos;
)
)

)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Action</label>
